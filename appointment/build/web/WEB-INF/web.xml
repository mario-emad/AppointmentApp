<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>DoctorRegisterS</servlet-name>
        <servlet-class>com.servlets.DoctorRegisterS</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DoctorLoginS</servlet-name>
        <servlet-class>com.servlets.DoctorLoginS</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PatientRegisterS</servlet-name>
        <servlet-class>com.servlets.PatientRegisterS</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PatientLoginS</servlet-name>
        <servlet-class>com.servlets.PatientLoginS</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReserveS</servlet-name>
        <servlet-class>com.servlets.ReserveS</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DoctorRegisterS</servlet-name>
        <url-pattern>/DoctorRegisterS</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DoctorLoginS</servlet-name>
        <url-pattern>/DoctorLoginS</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PatientRegisterS</servlet-name>
        <url-pattern>/PatientRegisterS</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PatientLoginS</servlet-name>
        <url-pattern>/PatientLoginS</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReserveS</servlet-name>
        <url-pattern>/ReserveS</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
